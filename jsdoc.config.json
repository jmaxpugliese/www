{
	"tags": {
		"allowUnknownTags": true,
	  "dictionaries": ["jsdoc","closure"]
	},
	"source": {
	  "include": ["src"],
		"includePattern": ".+\\.js(doc)?$",
	  "exclude": ["src/vendor"]
	},
	"plugins": [],
	"templates": {
		"default": {
			"useLongnameInNav": true
		}
	},
	"opts": {
		"destination": "./docs",
		"recurse": "true"
	}
}
